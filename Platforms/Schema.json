{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "required": false,
    "properties": {
        "FileExtensions": {
            "type": "array",
            "required": true,
            "items": {
                "type": "string",
                "required": true
            }
        },
        "Images": {
            "type": "object",
            "required": true,
            "properties": {
                "snowflake_img_platform_logo": {
                    "type": "string",
                    "required": false
                },
                "snowflake_img_platform_productshot": {
                    "type": "string",
                    "required": false
                }
            }
        },
        "Metadata": {
            "type": "object",
            "required": true,
            "properties": {
                "snowflake_platform_company": {
                    "type": "string",
                    "required": false
                },
                "snowflake_platform_releasedate": {
                    "type": "string",
                    "required": false
                },
                "snowflake_platform_shortname": {
                    "type": "string",
                    "required": false
                }
            }
        },
        "Name": {
            "type": "string",
            "required": true
        },
        "PlatformId": {
            "type": "string",
            "required": true
        },
        "RomIdentifierPlugin": {
            "type": "string",
            "required": true
        }
    }
}